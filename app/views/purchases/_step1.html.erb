<div role="tabpanel" class="tab-pane active" id="step1">
  <div class="form-group clearfix">
    <div class="col-xs-12">
      <div class="col-md-6">
        <%= f.label(:move_out_location, "Move Out", ) %>
        <% if current_user.schoolname == "Albright College" %>
        <%= f.select(:move_out_location, options_for_select(@locations.map{ |s| [s, s.tr(' ', '').underscore]}), class: 'form-control', required: true) %>
        <% elsif current_user.schoolname == "Palm Beach Atlantic" %>
        <%= f.select(:move_out_location, ["Baxter Hall", "Coastal Towers Apartments", "Johnson Hall", "Oceanview Hall", "Weyenberg Hall", "Rinker Hall", "Flagler Towers Apartments", "Lakeview Apartments", "Samaritan Gardens Apartments", "Mango Apartments"], class: 'form-control', required: true) %>
        <% elsif current_user.schoolname == "Kutztown University" %>
        <%= f.select(:move_out_location, ["Beck Hall", "Berks Hall", "Bonner Hall", "Deatrick Hall", "Dixon Hall", "Gold Bear Village South", "Gold Bear Village West", "Honors Building", "Johnson Hall", "Lehigh Hall", "Rothermel hall", "Schuyl Hall", "University Place"], class: 'form-control', required: true) %>
        <% end %>

      </div>
      <div class="col-md-6">
        <%= f.label(:move_out_room, "Room #", class: 'block') %>
        <%= f.text_field(:move_out_room,class: 'form-control', required: true) %>
      </div>
    </div>
    <div class="col-xs-12">
      <div class="col-md-6">
        <%= f.label(:move_in_location, "Move In") %>
        <% if current_user.schoolname == "Albright College" %>
        <%= f.select(:move_in_location, options_for_select(@locations.map{ |s| [s, s.tr(' ', '').underscore]}), class: 'form-control', required: true) %>
        <% elsif current_user.schoolname == "Palm Beach Atlantic" %>
        <%= f.select(:move_out_location, ["Baxter Hall", "Coastal Towers Apartments", "Johnson Hall", "Oceanview Hall", "Weyenberg Hall", "Rinker Hall", "Flagler Towers Apartments", "Lakeview Apartments", "Samaritan Gardens Apartments", "Mango Apartments"], class: 'form-control', required: true) %>
        <% elsif current_user.schoolname == "Kutztown University" %>
        <%= f.select(:move_out_location, ["Beck Hall", "Berks Hall", "Bonner Hall", "Deatrick Hall", "Dixon Hall", "Gold Bear Village South", "Gold Bear Village West", "Honors Building", "Johnson Hall", "Lehigh Hall", "Rothermel hall", "Schuyl Hall", "University Place"], class: 'form-control', required: true) %>
        <% end %>
      </div>
      <div class="col-md-6">
        <%= f.label(:move_in_room, "Room #", class: 'block') %>
        <%= f.text_field(:move_in_room, class: 'form-control', required: true) %>
      </div>
      <div class="col-md-6">
        <%= f.label(:move_out_date, "move out", class: 'block') %>
        <%= f.date_field(:move_out_date, class: 'form-control', required: true) %>
      </div>
      <div class="col-md-6">
        <%= f.label(:move_in_date, "move in", class: 'block') %>
        <%= f.date_field(:move_in_date, class: 'form-control', required: true) %>
      </div>

    </div>
  </div>
</div>
